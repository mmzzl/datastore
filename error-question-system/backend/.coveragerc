[run]
source = .
omit = 
    */migrations/*
    */venv/*
    */tests/*
    manage.py
    */settings/*
    config/test_settings.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov